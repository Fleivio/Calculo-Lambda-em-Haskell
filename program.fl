btw implementa fibonacci
fn fib = /x -> if (x == 0) || (x == 1) 
              then 1
              else (fib (x - 1)) + (fib (x - 2));
           
btw fatorial
fn fact = /x -> if (x == 0) 
              then 1 
              else x * (fact (x - 1));

fn snd = /x,y -> y;

fn even = /x -> if (x == 0) 
                then true
                else
                    if (x == 1) then false
                    else even (x - 2);

btw loop infinito
fn infinity = /x -> 1 + (infinity x);


